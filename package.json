{
  "name": "bazel-build-event-viewer-sk",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "genprotobuf": "pbjs -t static-module -w es6 static/build_event_stream.proto -o ./src/lib/generated/build_event_stream_proto.js && pbts -o ./src/lib/generated/build_event_stream_proto.d.ts ./src/lib/generated/build_event_stream_proto.js",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.6",
    "@eslint/js": "^9.20.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.17.1",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^15.14.0",
    "prettier": "^3.5.0",
    "prettier-plugin-svelte": "^3.3.3",
    "protobufjs-cli": "^1.1.3",
    "svelte": "^5.19.9",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.23.0",
    "vite": "^6.1.0",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "@andypf/json-viewer": "^2.1.7",
    "ansi_up": "^6.0.2",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "moment": "^2.30.1",
    "protobufjs": "^7.4.0"
  }
}
